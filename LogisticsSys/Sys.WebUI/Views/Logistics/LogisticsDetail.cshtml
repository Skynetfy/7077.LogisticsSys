@using Sys.Entities
@{
    Layout = null;
}

<ul class="list-unstyled timeline" id="viewdemailul" style="border-top: 1px solid #e8e8e8;margin-top: 20px;">
    @{
        var data = ViewData["LogisticsInfoList"] as List<SysLogisticsInfo>;
        if (data != null)
        {
            foreach (var item in data)
            {
                <li>
                    <div class="block">

                        <div class="block_content">
                            <h2 class="title">
                                @item.LogisticsDesc
                            </h2>
                            <div class="byline">
                                <span>@item.UpdateDate.ToString("yyyy-MM-dd hh:mm:ss")</span> 
                            </div>
                           
                        </div>
                    </div>
                </li>
            }
        }
    }
    
</ul>
<script>
    $(function() {
        $('#viewdemailul li:first h2').addClass('last');
    })
</script>